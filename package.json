    "@fastify/autoload": "^5.0.0",
    "@fastify/sensible": "^4.1.0",
    "fastify": "^4.9.2",
    "fastify-cli": "^4.4.0",
    "fastify-cron": "^1.3.1",
    "fastify-plugin": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.8",
